{
  "name": "MockBcx",
  "version": "1.0.0",
  "description": "MockBcx Token with Hardhat",
  "main": "index.js",
  "author": "mastervectormaster <vectormaster123456789@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.11.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3"
  },
  "scripts": {
    "clean": "npx hardhat clean",
    "deploy:ropsten": "npx hardhat run --network ropsten scripts/deploy.js",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deploy.js",
    "deploy:hardhat": "npx hardhat run --network hardhat scripts/deploy.js",
    "deploy:ganache": "npx hardhat run --network ganache scripts/deploy.js",
    "deploy:bscTest": "npx hardhat run --network bscTest scripts/deploy.js",
    "verifyContract:ropsten": "npx hardhat verifycontract --network ropsten --show-stack-traces",
    "verifyContract:rinkeby": "npx hardhat verifycontract --network rinkeby --show-stack-traces",
    "verifyContract:goerli": "npx hardhat verifycontract --network goerli --show-stack-traces"
  }
}
